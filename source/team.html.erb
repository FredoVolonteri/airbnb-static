<div class="container text-center padded">
  <h1>Our team</h1>
  <ul class="list-inline text-center">
  <% ["Fredo", "Alex"].each do |member| %>
    <li>
      <%= image_tag "#{member}.png", class: "avatar-large" %>
      <h3><%= member.capitalize %></h3>
    </li>
  <% end %>
</ul>

  <h3 class="padded">

    <% link_to "mailto:contact@airbnb.com" do %>
      <i class="fa fa-envelope"></i> contact@airbnb.com
    <% end %>
  </h3>
</div>


